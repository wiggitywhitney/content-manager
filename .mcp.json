{
  "mcpServers": {
    "commit-story": {
      "type": "stdio",
      "command": "node",
      "args": ["node_modules/commit-story/src/mcp/server.js"],
      "env": {}
    },
    "coderabbitai": {
      "command": "npx",
      "args": ["coderabbitai-mcp@latest"],
      "env": {
        "GITHUB_PAT": "${GITHUB_TOKEN}"
      }
    }
  }
}